{
  "name": "api-pengaduan",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./src/bin/www.js --exec babel-node",
    "clean": "rmdir /q /s dist && mkdir dist",
    "build": "npm run clean &&  babel ./src --out-dir dist --copy-files",
    "start": "node ./dist/bin/www.js",
    "serve": "SET DEBUG=api-pengaduan:* & npm run dev"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "nodemon": "^2.0.15"
  }
}
